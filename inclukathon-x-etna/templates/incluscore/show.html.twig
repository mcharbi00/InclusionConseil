{% extends 'base.html.twig' %}

{% block title %}Incluscore{% endblock %}

{% block body %}
    <h1>Incluscore</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ incluscore.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ incluscore.name }}</td>
            </tr>
            <tr>
                <th>SmallName</th>
                <td>{{ incluscore.smallName }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ incluscore.enabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CanBePublic</th>
                <td>{{ incluscore.canBePublic ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ incluscore.description }}</td>
            </tr>
            <tr>
                <th>IsInclucard</th>
                <td>{{ incluscore.isInclucard ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>InclucardColor</th>
                <td>{{ incluscore.inclucardColor }}</td>
            </tr>
            <tr>
                <th>IncluscoreColor</th>
                <td>{{ incluscore.incluscoreColor }}</td>
            </tr>
            <tr>
                <th>SecondIncluscoreColor</th>
                <td>{{ incluscore.secondIncluscoreColor }}</td>
            </tr>
            <tr>
                <th>DisplayNewStudentNumber</th>
                <td>{{ incluscore.displayNewStudentNumber ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_incluscore_index') }}">back to list</a>

    <a href="{{ path('app_incluscore_edit', {'id': incluscore.id}) }}">edit</a>

    {{ include('incluscore/_delete_form.html.twig') }}
{% endblock %}
